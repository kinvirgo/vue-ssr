{
  "name": "vue-ssr",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "git@github.com:kinvirgo/vue-ssr.git",
  "author": "jinyujiang <jinyujiang@sfmail.sf-express.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node server.js",
    "serve": "npm run build:dev && node server.js",
    "build:dev": "rimraf dist && npm run build:dev-client && npm run build:dev-server",
    "build:dev-client": "webpack --config build/webpack.client.conf.js --progress --hide-modules",
    "build:dev-server": "webpack --config build/webpack.server.conf.js --progress --hide-modules",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.conf.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.conf.js --progress --hide-modules",
    "start": "cross-env NODE_ENV=production node server.js"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "koa": "^2.13.0",
    "koa-favicon": "^2.1.0",
    "koa-router": "^7.2.1",
    "koa-static": "^5.0.0",
    "lru-cache": "^5.1.1",
    "vue-server-renderer": "^2.6.11"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "autoprefixer": "^9.8.4",
    "axios": "^0.21.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "http-proxy-middleware": "^1.0.5",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-hot-middleware": "^1.0.3",
    "koa2-proxy-middleware": "^0.0.4",
    "mini-css-extract-plugin": "^0.9.0",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.1",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.3.4",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-hashed-chunk-id-plugin": "^1.0.3-beta",
    "webpack-merge": "^5.0.7",
    "webpack-node-externals": "^1.7.2"
  }
}
